<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>netstandard2.0;net6.0;net7.0;net8.0</TargetFrameworks>
		<LangVersion>latest</LangVersion>
		<Nullable>enable</Nullable>
		<WarningsAsErrors>nullable</WarningsAsErrors>
		<EmitCompilerGeneratedFiles>true</EmitCompilerGeneratedFiles>
	</PropertyGroup>

	<ItemGroup>
	  <PackageReference Include="DTOMaker.Core" Version="0.9.5" />
	  <PackageReference Include="DTOMaker.MemBlocks" Version="0.9.2" />
	  <PackageReference Include="DTOMaker.MessagePack" Version="0.9.3" />
	  <PackageReference Include="MessagePack" Version="3.0.134-beta" />
	</ItemGroup>

</Project>
